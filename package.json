{
  "name": "@rapidevelop/xml2json",
  "version": "1.3.0",
  "description": "XML2JSON is a library to convert a XML file or XML text string to JSON.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "git:commit": "git-cz",
    "version:major": "npm version major --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:patch": "npm version patch --no-git-tag-version",
    "test:type": "tsc --noEmit",
    "test:type:watch": "npm run test:type -- --watch",
    "build:type": "tsc --emitDeclarationOnly",
    "build:js": "gulp build:js",
    "build:js:watch": "gulp build:w",
    "build": "npm run build:type && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapidevelop/xml2json.git"
  },
  "keywords": [],
  "author": "Iman Vafadarnezhad",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/rapidevelop/xml2json/issues"
  },
  "homepage": "https://github.com/rapidevelop/xml2json#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^12.7.1",
    "@types/request": "^2.48.2",
    "@types/sax": "^1.2.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "request": "^2.88.0",
    "sax": "^1.2.4"
  }
}
